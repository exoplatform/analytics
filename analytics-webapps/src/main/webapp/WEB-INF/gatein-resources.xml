<gatein-resources xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.exoplatform.org/xml/ns/gatein_resources_1_4 http://www.exoplatform.org/xml/ns/gatein_resources_1_4" xmlns="http://www.exoplatform.org/xml/ns/gatein_resources_1_4">

  <portal-skin>
    <skin-name>Enterprise</skin-name>
    <skin-module>analytics</skin-module>
    <css-path>/skin/css/analytics.css</css-path>
    <css-priority>200</css-priority>
  </portal-skin>

  <portlet>
    <name>AnalyticsPortlet</name>
    <module>
      <script>
        <minify>false</minify>
        <path>/js/analytics.bundle.js</path>
      </script>
      <depends>
        <module>vue</module>
      </depends>
      <depends>
        <module>vuetify</module>
      </depends>
      <depends>
        <module>eXoVueI18n</module>
      </depends>
      <depends>
        <module>jquery</module>
        <as>$</as>
      </depends>
      <depends>
        <module>fetchPolyfill</module>
      </depends>
      <depends>
        <module>customEventPolyfill</module>
      </depends>
      <depends>
        <module>iePolyfills</module>
      </depends>
      <depends>
        <module>eCharts</module>
      </depends>
    </module>
  </portlet>

  <portlet>
    <name>AnalyticsDashboardBreadcrumb</name>
    <module>
      <script>
        <minify>false</minify>
        <path>/js/breadcrumb.bundle.js</path>
      </script>
      <depends>
        <module>vue</module>
      </depends>
      <depends>
        <module>vuetify</module>
      </depends>
      <depends>
        <module>eXoVueI18n</module>
      </depends>
      <depends>
        <module>jquery</module>
        <as>$</as>
      </depends>
      <depends>
        <module>extensionRegistry</module>
      </depends>
    </module>
  </portlet>

  <portlet>
    <name>StatisticsCollection</name>
    <module>
      <script>
        <minify>false</minify>
        <path>/js/statistic-collection.js</path>
      </script>
      <depends>
        <module>jquery</module>
        <as>$</as>
      </depends>
      <depends>
        <module>commons-cometd3</module>
        <as>cCometd</as>
      </depends>
    </module>
  </portlet>

</gatein-resources>
